export declare class SetLocaleService {
    private document;
    constructor(document: Document);
    getCookieName(): string;
    setLocale(locale: string): void;
    /**
     * @deprecated Will be removed in v7.0.0
     */
    protected setLocaleLegacy(locale: string): Promise<{
        status: number;
        statusText: string;
        headers: {};
        body: string;
    }>;
}
