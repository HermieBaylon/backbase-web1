import { ItemModel } from '../../models/item-model';
import { ObservableTree } from '../../models/tree';
import { ItemRegistryService } from '../item-registry.service';
/**
 * AreaComponent is a wrapper for ChromeComponent,
 * which defines the area for dynamic components rendering
 */
export declare class AreaComponent {
    private itemRegistry;
    /**
     * name attribute of the area,
     * represented also as a "data-area"
     * attribute in the corresponding node
     */
    name: string;
    children: Array<ObservableTree<ItemModel>>;
    constructor(itemRegistry: ItemRegistryService);
    /**
     * @internal
     */
    isModule(model: ItemModel): boolean;
}
