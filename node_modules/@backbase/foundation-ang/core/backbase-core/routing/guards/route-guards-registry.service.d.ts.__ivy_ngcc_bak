import { InjectionToken, Type } from '@angular/core';
import { CanActivate, CanActivateChild } from '@angular/router';
export declare type RouteGuardKey = string;
export interface RouteGuardsConfig {
    canActivate?: {
        [key: string]: Type<CanActivate>;
    };
    canActivateChild?: {
        [key: string]: Type<CanActivateChild>;
    };
}
export declare const CONFIG_ROUTE_GUARDS: InjectionToken<RouteGuardsConfig>;
/**
 * @deprecated Will be removed in v7.0.0
 */
export declare class RouteGuardsRegistryService {
    readonly canActivate: Map<RouteGuardKey, Type<CanActivate>>;
    readonly canActivateChild: Map<RouteGuardKey, Type<CanActivateChild>>;
    constructor(configs: Array<RouteGuardsConfig>);
}
