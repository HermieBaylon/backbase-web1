import { InjectionToken } from '@angular/core';
export declare type ClassId = string;
export declare type OutputName = string;
export declare type InputName = string;
export interface ComponentOutputConfig {
    classId: ClassId;
    outputName: OutputName;
}
export interface ComponentInputConfig {
    classId: ClassId;
    inputName?: InputName;
}
export interface CommunicationFlowConfig {
    output: ComponentOutputConfig;
    input: ComponentInputConfig;
    mapPayload?: (payload: any) => any;
}
export interface Flow {
    output: OutputName;
    input?: InputName;
    map?: (payload: any) => any;
}
export declare const CONFIG_FLOWS: InjectionToken<CommunicationFlowConfig[][]>;
export declare class FlowsService {
    private readonly transitions;
    constructor(flowsConfig: // eslint-disable-next-line @typescript-eslint/array-type
    Array<CommunicationFlowConfig>[] | null);
    private addFlow;
    getFlows(from: ClassId, fromOutput: OutputName, to: Array<ClassId>, toInput: InputName | undefined): Array<Flow>;
}
